{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <21.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build-only",
    "build-only": "vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 20",
    "lint-fix": "eslint src --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "prebuild": "npm run type-check",
    "postbuild": "npm run validate-build",
    "validate-build": "test -d dist && test -f dist/index.html || (echo 'Build validation failed' && exit 1)",
    "server": "node server/index.js",
    "start": "npm run server",
    "audit:accessibility": "echo \"Manual accessibility audit: Open DevTools > Lighthouse > Accessibility > Generate Report\" && echo \"Target: Score >= 95, No contrast violations\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.54.0",
    "@types/node": "^24.2.1",
    "@vitejs/plugin-react": "^4.3.1",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.10",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet-async": "^1.0.1",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-debug-cache": "^1.0.0",
    "postcss": "^8.4.35",
    "sqlite3": "^5.1.7",
    "typescript-eslint": "^8.39.0"
  }
}
