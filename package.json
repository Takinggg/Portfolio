{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <21.0.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "server": "node server/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node server/index.js",
    "build": "npm run type-check && npm run build-only",
    "build-only": "vite build",
    "build:production": "NODE_ENV=production npm run build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 20",
    "lint-fix": "eslint src --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "preview:production": "npm run build && npm run preview",
    "prebuild": "npm run type-check",
    "postbuild": "npm run validate-build",
    "validate-build": "test -d dist && test -f dist/index.html || (echo 'Build validation failed' && exit 1)",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/",
    "clean": "rm -rf dist node_modules/.vite",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.54.0",
    "@types/quill": "^2.0.14",
    "@types/react-modal": "^3.16.3",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "framer-motion": "^12.23.12",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.344.0",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-modal": "^3.16.3",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.1",
    "three": "^0.179.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.10",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.54.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet-async": "^1.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-debug-cache": "^1.0.0",
    "postcss": "^8.4.35",
    "typescript-eslint": "^8.39.0"
  }
}
